{
  "scopeName": "text.markdown-themeable-pdf",
  "injectionSelector": "source.gfm, text.md",
  "patterns": [
    {
      "match": "\\[(x| )\\]",
      "name": "variable.unordered.list.text.markdown-themeable-pdf.checkbox"
    },
    {
      "match": "^@\\[toc\\]$",
      "name": "markup.heading.heading-1.text.markdown-themeable-pdf.toc"
    },
    {
      "match": "^<div class=(\"|')page-break(\"|')></div>$",
      "name": "comment.hr.text.markdown-themeable-pdf.page-break"
    }
  ]
}
